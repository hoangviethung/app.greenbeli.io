extends ../layouts/layout

block var
	- pageTitle = "Liquidity | Defi x NFT"
block library-css
block css
	link(rel="stylesheet", href="./css/liquidity.css")
block library-js
block js
	script(src="./js/liquidity.js")
block main
	section.trade.liquidity
		.tabs.neutral-background-lighter
			.tabs__nav
				a.tabs__link(href="exchange.html")
					.tabs__icon
						i.fad.fa-sync
					.tabs__text Swap
				a.tabs__link.active(href="liquidity.html")
					.tabs__icon
						i.fad.fa-dollar-sign
					.tabs__text Liquidity Pool
			.tabs__content
				.tabs__content__header.trade__header
					.box-left
						h2.trade__title Your Liquidity
						p.trade__desc Remove liquidity to receive tokens back
					.box-right
						a.bt-circle.bt-circle--lg.txt-secondary(popup-to="settings_popup" href="javascript:;")
							i.fad.fa-sliders-h
						a.bt-circle.bt-circle--lg.txt-secondary(popup-to="recent_transactions_popup" href="javascript:;")
							i.fad.fa-history
				.tabs__content__body.trade__body
					.liquidity__item
						p.fs-1rem.fw-600.ta-c.txt-disable Don't see a pool you joined?
						.flex-alc-jcc.mt-1rem
							button.bt.bt-outline.primary Find other LP tokens
				.tabs__content__footer.trade__footer
					button.bt.bt-lg.bt-block.primary
						i.fal.fa-plus
						span Add Liquidity
