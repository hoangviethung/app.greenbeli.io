#confirm_buy_warrior_popup.confirm_liquidity_popup(style="display:none")
	.popup_wrapper
		.popup__header
			h2.popup__title You will receive
		.popup__body
			.confirm_liquidity__item
				.confirm_warrior__character
					img(src="./images/character_buy.png", alt="CHARACTER")
				h4.fs-1x4rem.ta-c.mb-1rem  BTC #20210623
				p.ta-c Warriors will be minted after adding liquidityFIWA will be burned. Player cannot clear liquidity to get token back
			.confirm_liquidity__item
				.flex-between-alc
					span CAKE Deposited
					.token__information
						img.token__icon(src="./images/FIWA.png", alt="FIWA")
						span.token__price 0.0696463
				.flex-between-alc
					span BUSD Deposited
					.token__information
						img.token__icon(src="./images/BTCB.png", alt="BTCB")
						span.token__price 1.0098
				.flex-between-alc
					span Rates
					div
						p 1 REM = 14.36 BUSD
						p 1 CAKE = 14.36 BUSD
				.flex-between-alc
					span Share of Pool:
					span 0.000005406%
		.popup__footer
			button.bt.bt-lg.bt-block.primary(popup-to="confirm_buy_warrior_loading_popup" href="javascript:;") Confirm Supply (Loading)
			button.bt.bt-lg.bt-block.primary(popup-to="confirm_buy_warrior_accept_popup" href="javascript:;") Confirm Supply (Accept)
			button.bt.bt-lg.bt-block.primary(popup-to="confirm_buy_warrior_reject_popup" href="javascript:;") Confirm Supply (Reject)