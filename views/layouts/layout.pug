					include ../components/confirm_buy_warrior_accept_popup
					// *** POPUP CONFIRM BUY WARRIOR REJECT *** //
					include ../components/confirm_buy_warrior_reject_popup
					// *** POPUP CONFIRM SETTINGS *** //
					include ../components/settings_popup
					// *** POPUP RECENT TRANSACTION *** //
					include ../components/recent_transactions_popup
		// *** OVERLAY *** //
		.overlay
		// *** FOOTER *** //
		block footer
			include footer
		// *** VENDORS JS *** //
		script(src="./vendors/jquery/jquery.js")
		script(src="./vendors/fancybox/jquery.fancybox.js")
		block library-js
		// *** JS *** //
		block js
		include js