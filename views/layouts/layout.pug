block var

<!DOCTYPE html>
html(lang="en")
	head
		// *** META *** //
		include meta
		// *** VENDORS CSS *** //
		link(rel="stylesheet", href="./vendors/fancybox/jquery.fancybox.css")
		block library-css
		// *** CSS *** //
		include css
		block css
		// *** FAVICON *** //
		link(rel="shortcut icon", href="favicon.ico", type="image/x-icon")
