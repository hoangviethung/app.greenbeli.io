					include ../components/select_token_popup
					// *** POPUP SELECT A LIQUIDITY *** //
					include ../components/select_liquidity_popup
					// *** POPUP CONFIRM LIQUIDITY *** //
					include ../components/confirm_liquidity_popup
					// *** POPUP SELECT A LIQUIDITY LOADING *** //
					include ../components/confirm_liquidity_loading_popup
					// *** POPUP SELECT A LIQUIDITY REJECT *** //
					include ../components/confirm_liquidity_reject_popup
